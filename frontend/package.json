{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:serve": "npx http-server -p 8080 -c-1 dist/angular-boilerplate/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format-staged": "pretty-quick --staged",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.0.5",
    "@angular/common": "19.0.5",
    "@angular/compiler": "19.0.5",
    "@angular/core": "19.0.5",
    "@angular/forms": "19.0.5",
    "@angular/platform-browser": "19.0.5",
    "@angular/platform-browser-dynamic": "19.0.5",
    "@angular/router": "19.0.5",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@ng-select/ng-select": "^14.9.0",
    "@ngneat/overview": "6.1.1",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@ngxpert/hot-toast": "4.2.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "ng-bootstrap-icons": "^13.1.2-canary.1",
    "pdf-lib": "^1.17.1",
    "rxjs": "~7.8.0",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular-eslint/schematics": "^19.1.0",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "19.0.5",
    "@angular/language-service": "19.0.5",
    "@angular/localize": "19.0.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-rocket/scripts": "^5.2.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.5.5",
    "class-transformer": "^0.5.1",
    "globals": "^15.9.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.6.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
